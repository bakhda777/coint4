BYBIT_ENV=demo
BYBIT_API_KEY=your_demo_api_key
BYBIT_API_SECRET=your_demo_api_secret
BYBIT_CATEGORY=linear
BYBIT_ACCOUNT_TYPE=UNIFIED
BYBIT_SETTLE_COIN=USDT
BYBIT_POSITION_MODE=hedge
# If Bybit rejects hedge orders with "position idx not match position mode", set:
# BYBIT_POSITION_MODE=oneway
BYBIT_RECV_WINDOW=5000
BYBIT_MAX_RETRIES=3
BYBIT_RETRY_BACKOFF=1.0
BYBIT_MIN_NOTIONAL_BUFFER_PCT=0.05
BYBIT_KLINE_CACHE_SECONDS=60
BYBIT_STATE_PATH=artifacts/live/state.json
BYBIT_SYNC_ON_START=true
